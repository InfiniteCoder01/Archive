bool Is2power(int x) { return (!(x & (x - 1))); }
long ALIGN_DOWN(long x, long align) { return (x & ~(align - 1)); }
long ALIGN_UP(long x, long align) { return ((x & (align - 1)) ? ALIGN_DOWN(x, align) + align : x); }


